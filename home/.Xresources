!! URxvt Appearance
URxvt.font:           xft:Cozette:style=Medium:pixelsize=16
URxvt.boldFont:       xft:Cozette:style=Medium:pixelsize=16
URxvt.italicFont:     xft:Cozette:style=Medium:pixelsize=16
URxvt.boldItalicFont: xft:Cozette:style=Medium:pixelsize=16

URxvt.letterSpace:     1 
URxvt.lineSpace:       1
URxvt.internalBorder:  15 
URxvt.cursorBlink:     true
URxvt.cursorUnderline: false
URxvt.urgentOnBell:    true
URxvt.scrollBar:       false

!! URxvt Transparency
URxvt.depth:      32
URxvt.background: [98]#0c0c0f 

!! URxvt copy/paste
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.perl-ext-common: default

URxvt.iso14755:    false
URxvt.iso14755_52: false
